		>
			<div>
				<Header title="Sobre Mi" />
			</div>
			<div>
				<Side />
			</div>
			<div>
				<About />
			</div>
			<div>
				<Projects />
			</div>
			<div>
				<TimeLines />
			</div>
			<div>
				<Contact />
			</div>
			<div>
				<Footer />
			</div>
		</div>
	</main>
</Layout>

